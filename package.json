{
  "name": "mvb-prototype-hybrid-webapp",
  "version": "1.4.0-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "cp ./config/* ./src/assets/config/ & ng serve --host 0.0.0.0 --disable-host-check",
    "build": "cp ./config/* ./src/assets/config/ & ng build --prod & build-storybook -s public",
    "test": "cp ./config/* ./src/assets/config/ & ng test",
    "lint": "cp ./config/* ./src/assets/config/ & ng lint",
    "e2e": "cp ./config/* ./src/assets/config/ & ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "cp ./config/* ./src/assets/config/ & npm run docs:json & start-storybook -p 6006",
    "build-storybook": "cp ./config/* ./src/assets/config/ & build-storybook",
    "run-all": "cp ./config/* ./src/assets/config/ & node node_modules/serve/bin/serve.js -l 4200 dist/test-angular & node node_modules/serve/bin/serve.js -l 6006 storybook-static"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.7",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.7",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/router": "~9.0.7",
    "@angular/service-worker": "~9.0.7",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "bootstrap": "^4.4.1",
    "ngx-bootstrap": "^5.6.1",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@angular/language-service": "~9.0.7",
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/angular": "^5.3.18",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.37",
    "babel-loader": "^8.1.0",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "protractor": "^5.4.4",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
